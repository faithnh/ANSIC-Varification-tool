.RI "void \fBgetFreememInfo\fP (\fBFREEMEMINFO\fP **freememinfo, \fBAST\fP *call_function, FUNCTION_INFORMATION_LIST *function_information_list, VARIABLE_TABLE_LIST *vtlist, \fBAST\fP *target_statement)"
.br
.ti -1c
